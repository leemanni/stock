<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leemanni.vo.LoginVO">
	<!-- id : sql 명령을 식별한다. -->
	<!-- parameterType : sql 명령으로 전달되는 데이터의 타입을 적는다. -->
	<!-- resultType : sql 명령 실행결과 리턴되는 데이터의 타입을 적는다. -->
	
	<select id="checkLogin" parameterType="logVO" resultType="int">
		select count(*) from logindata where id = #{id} and pw = #{pw}
	</select>
	
</mapper>















